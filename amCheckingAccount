public class ammCheckingAccount{
  public String name;
  private int balance;
  private String id;
  
  public CheckingAccount(String inputName, int inputBalance, String inputId){
    name = inputName;
    balance = inputBalance;
    id = inputId;
  }  

    public int getBalance(){
    return this.balance;
  }
    //accessor because we are returning the value by accessing it

    public void setBalance(int newBalance){
      balance = newBalance;
    }
    //mutator because we ARE NOT returning the value, instead we are adjusting it
  
}
